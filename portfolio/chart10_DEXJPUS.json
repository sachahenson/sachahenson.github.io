{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Japanese Yen to US dollar spot exchange rate",
    "title": {
        "text": "Japanese Yen to US dollar spot exchange rate",
        "subtitle": "Noon buying rate of Japanese Yen for 1 US dollar",
        "subtitleFontStyle": "italic",
        "subtitleFontSize": 10,
        "anchor": "start"
    },
    "data": {
        "url": "https://raw.githubusercontent.com/sachahenson/sachahenson.github.io/main/portfolio/Skill%206/data-Fred-DEXJPUS.json",
        "format": {
            "type": "json",
            "property": "observations"
        }
    },
    "height": 300,
    "width": 300,
    "mark": {
        "type": "line",
        "point": false,
        "color": "red"
    },
    "encoding": {
        "x": {
            "field": "date",
            "type": "temporal",
            "axis": {
                "title": null,
                "grid": false,
                "labelSeparation": 10
            }
        },
        "y": {
            "field": "value",
            "type": "quantitative",
            "title": "",
            "scale": {
                "domain": [
                    0,
                    400
                ]
            },
            "axis": {
                "grid": true,
                "format": ".0s",
                "labelSeparation": 50
            }
        },
        "color": {
            "timeUnit": "year",
            "field": "date",
            "title": "Year",
            "scale": {
                "scheme": "turbo"
            }
        },
        "tooltip": [
            {
                "field": "year_date",
                "type": "temporal",
                "title": "Year"
            },
            {
                "field": "value",
                "type": "quantitative",
                "title": "JPY to USD",
                "format": ".2f"
            }
        ]
    }
}